{
    "name": "laravelir/newsletters",
    "description": "laravel newsletters package",
    "homepage": "https://github.com/laravelir/newsletters",
    "type": "library",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel support",
        "laravel newsletters",
        "lumen packages",
        "laravel packages",
        "lumen support"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com",
            "role": "maintaner",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravelir\\Newsletters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelir\\Newsletters\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NewslettersServiceProvider"
            ],
            "aliases": {
                "Newsletters": "Laravelir\\Newsletters\\Facades\\NewslettersFacade"
            }
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
